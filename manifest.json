{
    "name": "Developer Search",
    "description": "Advanced Search features for developers a click away",
    "version": "0.1",
    "manifest_version": 3,
    "icons": {
        "128": "128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.google.com/*"
            ],
            "css": [
                "jquery-ui.min.css",
                "output.css"
            ],
            "js": [
                "jquery-3.6.0.min.js",
                "jquery-ui.min.js",
                "content-script.js"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "matches": [
                "https://www.google.com/*"
            ],
            "resources": [
                "main.html"
            ]
        }
    ]
}